class Solution:
    def removeNthFromEnd(self, head: Optional[ListNode], n: int) -> Optional[ListNode]:

        dummy = ListNode()
        dummy.next = head
        left = right = dummy

        # dummy ->1 ->2 ->3 ->4 ->5
        # left
        #           right  
        for _ in range(n):
            right = right.next
        
        # dummy ->1 ->2 ->3 ->4 ->5
        #               left
        #                       right  
        while right.next:
            left = left.next
            right = right.next

        left.next = left.next.next

        return dummy.next
